{
  "name": "FindJAV",
  "version": "0.2",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "browser_action": {
    "name": "FindJAV",
    "default_icon": "icon.png"
  },
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "content_scripts": [
    {
        "matches": ["http://www.dmm.co.jp/digital/videoa/*"],
        "css": ["css/bootstrap.css", "css/content.css"],
        "js": ["js/content.js"]
    }
  ]
}
